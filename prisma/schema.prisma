generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int     @id @default(autoincrement())
  nama     String
  divisi   String
  email    String  @unique
  password String
  isAdmin  Boolean @default(false)
}

model pendis {
  id           Int       @id @default(autoincrement())
  status       String
  noRegister   String    @unique(map: "dataPendis_noRegister_key")
  tglRegister  DateTime? @db.Date
  pemohon      String
  penerima     String
  nik          String    @db.VarChar(16)
  alamat       String
  kecamatan    String
  desa         String
  program      String
  asnaf        String
  rpPermohonan String
  rpRealisasi  String
  tglRealisasi DateTime? @db.Date
  uploadData   String
  keterangan   String
  createdAt    DateTime  @default(now())
  updatedAt    DateTime? @db.Date
}
